{
  "name": "angular-tour-of-heroes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "cypress": "wait-on http://localhost:4200 && cypress run",
    "build": "ng build angular-tour-of-heroes --prod --output-hashing=none",
    "package": "gulp package",
    "test": "ng test",
    "cypress:test": "cypress run",
    "lint": "ng lint",
    "build:prod":"ng build --prod",
    "start:prod": "http-server ./dist -p 4200",
    "build-and-deploy:prod": "run-s build:prod start:prod",
    "e2e": "start-server-and-test build-and-deploy:prod http://localhost:4200 cypress:test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/elements": "^8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@capacitor/android": "^1.0.0",
    "@capacitor/cli": "^1.0.0",
    "@capacitor/core": "^1.0.0",
    "@types/intro.js": "^2.4.4",
    "angular-in-memory-web-api": "^0.8.0",
    "document-register-element": "^1.7.2",
    "http-server": "^0.11.1",
    "intro.js": "^2.9.3",
    "jquery": "^3.5.0",
    "ng-animate": "^0.3.4",
    "npm-run-all": "^4.1.5",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "wait-on": "^3.3.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cypress": "^3.4.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "start-server-and-test": "^1.10.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
